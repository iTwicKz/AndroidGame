
public class Enemy {
	private int damageInflicting;
	private int health;
	private String name;
	private int worth;
	private int x;
	private int y;
	private int range;
	
	Enemy(){
		damageInflicting = 0;
		health = 0;
		name = "";
		worth = 0;
		x = 1280;
		y = 780;
		range = 0;
	}
	
	Enemy(int damageInflicting, int health, String name, int worth, int x, int y, int range){
		this.damageInflicting = damageInflicting;
		this.health = health;
		this.name = name;
		this.worth = worth;
		this.x = x;
		this.y = y;
		this.range = range;
	}
	
	public int inflictDamage(){
		if(x < 12)	return damageInflicting;
		return 0;
	}
	
	public void takeDamage(int hitDamage){
		this.health = health - hitDamage;
		if(health == 0){
			dieEnemy();
		}
	}
	
	public int dieEnemy(){
		
	}
	
	public void move(){
		
	}
	
	
	
	
	

}
